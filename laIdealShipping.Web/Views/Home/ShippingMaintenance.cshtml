@model IEnumerable<laIdealShipping.Entities.Shipping>

@{
    ViewBag.Title = "Shipping Maintenance";
}

<h2>Shipping Maintenance</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.nextShippingDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.salida)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Status)
                </th>
                <th></th>
            </tr>
        </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr class="info">
                <td>
                    @Html.DisplayFor(model => item.nextShippingDate)
                </td>
                <td>
                    @Html.DisplayFor(model => item.salida)
                </td>
                <td>
                    @Html.DisplayFor(model => item.Status)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }
    </tbody>
</table>
